{
  "name": "gridsome-remark-embed-code",
  "version": "1.1.0",
  "description": "Gridsome markdown remark transformer plugin to embed the contents total or partial of specified files as code snippets.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/Gonzalo2310/gridsome-remark-embed-code/tree/master",
  "repository": "https://github.com/Gonzalo2310/gridsome-remark-embed-code/tree/master/#readme",
  "keywords": [
    "gridsome",
    "remark",
    "transformer",
    "plugin",
    "code",
    "snippets"
  ],
  "author": "Infogon",
  "license": "MIT",
  "dependencies": {
    "normalize-path": "^3.0.0",
    "unist-util-visit": "^2.0.3",
    "vfile": "^4.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "jest-runner-remark": "^1.0.22",
    "remark": "^13.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js"
  }
}
